{
  "name": "@folio/plugin-find-license",
  "version": "5.0.0",
  "description": "License-finder for Stripes",
  "repository": "folio-org/ui-plugin-find-license",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "stripes": {
    "actsAs": [
      "plugin"
    ],
    "pluginType": "find-license",
    "displayName": "ui-plugin-find-license.meta.title",
    "okapiInterfaces": {
      "licenses": "1.0 2.0 3.0"
    },
    "stripesDeps": [
      "@folio/stripes-erm-components"
    ],
    "permissionSets": [
      {
        "permissionName": "ui-plugin-find-license.search",
        "displayName": "Find License Plugin: Search licenses",
        "visible": true,
        "subPermissions": [
          "licenses.licenses.view",
          "licenses.orgs.view",
          "licenses.refdata.view"
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "echo 'placeholder. no tests implemented'"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^5.2.0",
    "@folio/stripes": "^5.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^6.2.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-intl": "^4.5.1"
  },
  "dependencies": {
    "@folio/stripes-erm-components": "^4.0.0",
    "classnames": "^2.2.5",
    "dom-helpers": "^3.4.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^5.0.0",
    "react": "*",
    "react-intl": "^4.5.1"
  }
}
